{
  "name": "bsky-bot",
  "version": "1.0.0",
  "description": "A template repo for building Bluesky bots",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "prettier . --write --cache && eslint src/ --cache",
    "build": "bun build ./src/index.ts --outdir ./dist --minify",
    "watch": "tsc --watch",
    "start": "bun .",
    "dev": "bun run build && bun run start"
  },
  "keywords": [
    "bsky",
    "bluesky",
    "atproto",
    "bot"
  ],
  "author": {
    "name": "Phil Nash",
    "email": "philnash@gmail.com",
    "url": "https://philna.sh/"
  },
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/bun": "^1.1.13",
    "bun": "^1.1.34",
    "eslint": "^9.15.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.15.0"
  },
  "dependencies": {
    "@atproto/api": "^0.13.16",
    "dayjs": "^1.11.13",
    "zod": "^3.23.8"
  }
}
