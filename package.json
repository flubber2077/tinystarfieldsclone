{
  "name": "bsky-bot",
  "version": "1.0.0",
  "description": "A template repo for building Bluesky bots",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "npx prettier . && eslint src/",
    "build": "bun build ./src/index.ts --outdir ./dist --minify",
    "watch": "tsc --watch",
    "start": "bun .",
    "dev": "bun run build && bun run start"
  },
  "keywords": [
    "bsky",
    "bluesky",
    "atproto",
    "bot"
  ],
  "author": {
    "name": "Phil Nash",
    "email": "philnash@gmail.com",
    "url": "https://philna.sh/"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "^1.1.13",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "bun": "^1.1.34",
    "eslint": "^8.57.1",
    "eslint-plugin-sonarjs": "^0.19.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@atproto/api": "^0.13.16",
    "zod": "^3.23.8"
  }
}
